    "chalk": "^5.0.0",
    "chokidar": "^4.0.0",
    "electron": "18.3.7",
    "esbuild": "^0.24.0",
    "eslint": "9.13.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-import-helpers": "2.0.1",
    "eslint-plugin-jsx-a11y": "6.10.1",
