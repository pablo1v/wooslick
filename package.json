    "babel-loader": "^8.2.2",
    "chalk": "^4.1.1",
    "chokidar": "^3.5.2",
    "electron": "13.2.3",
    "esbuild": "^0.12.15",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-import-helpers": "1.1.0",
